{
  "name": "xutl/laravel-aliyun",
  "description": "This is a Laravel expansion for the Aliyun.",
  "keywords": [ "laravel", "aliyun" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-simplexml": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "XuTL\\Aliyun\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "XuTL\\Aliyun\\AliyunServiceProvider"
      ],
      "aliases": {
        "Aliyun": "XuTL\\Aliyun\\Aliyun"
      }
    }
  },
  "prefer-stable": true
}
